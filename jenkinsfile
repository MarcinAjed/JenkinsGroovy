node {
    stage('SCM') {
	echo 'Gathering code from version control'
	git branch: '${branch}', url: 'https://github.com/MarcinAjed/JenkinsGroovy.git'
    }
    stage('Build') {
        echo 'Building  PRO....'
		sh 'dotnet --version'
		sh 'dotnet build ConsoleApp1'
		echo 'Building new feature'
    }
    stage('Test') {
        echo 'Testing....'
    }
    stage('Deploy') {
        echo 'Deploying....'
    }
}